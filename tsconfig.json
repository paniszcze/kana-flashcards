{
    "compilerOptions": {
        "target": "es5",
        "allowJs": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "es6",
        "moduleResolution": "node",
        "jsx": "preserve",
        "noImplicitAny": false,
        "noUncheckedIndexedAccess": true
    },
    "include": ["./src/"]
}
